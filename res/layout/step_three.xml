<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ColorPrimary">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="200dp"
        android:layout_height="300dp"
        android:src="@drawable/step_three"
        android:layout_below="@+id/textView30"
        android:layout_alignLeft="@+id/textView30"
        android:layout_alignStart="@+id/textView30"
        android:layout_marginTop="25dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:text="View people in the Introductions page\nBookmark and Poke someone interesting!"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/facebook"
        android:layout_below="@+id/textView30"
        android:layout_alignRight="@+id/textView30"
        android:layout_alignEnd="@+id/textView30"
        android:layout_marginTop="60dp" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/twitter"
        android:layout_below="@+id/imageView19"
        android:layout_alignLeft="@+id/imageView19"
        android:layout_alignStart="@+id/imageView19"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="-50dp" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/instagram"
        android:layout_below="@+id/imageView20"
        android:layout_alignLeft="@+id/imageView20"
        android:layout_alignStart="@+id/imageView20"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="10dp" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:src="@drawable/phone"
        android:layout_below="@+id/imageView21"
        android:layout_alignRight="@+id/imageView21"
        android:layout_alignEnd="@+id/imageView21"
        android:layout_marginTop="10dp"
        android:layout_marginRight="50dp" />

    <Button
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp"
        android:background="@drawable/gotit"
        android:text="GOT IT!"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:id="@+id/button3" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="It&apos;s a great ice-breaker!"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000"
        android:textSize="18sp"
        android:layout_below="@+id/imageView18"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp" />
</RelativeLayout>